<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Surefire suite" verbose="10" parallel="instances" thread-count="1" configfailurepolicy="continue">
    <listeners>
        <listener class-name="framework.listeners.TestLogger"/>
    </listeners>
    <test name="all">
        <packages>
            <package name="tests.*" />
        </packages>
    </test>
</suite>
